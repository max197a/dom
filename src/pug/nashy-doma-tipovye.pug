  // BEGIN SECTION :: head
  .head.head--houses
    .container.container--typical
      h2 Наши дома

  // END SECTION :: head

  // BEGIN SECTION :: typical
  section.typical
    .container.container--typical
      .typical__buttons
        a(href="/nashy-doma-tipovye.html").typical__btn.is-active типовые проекты
        a(href="/nashy-doma-gotovye.html").typical__btn готовые
        a(href="/blog-help.html").typical__btn строящиеся
        a(href="/blog-faq.html").typical__btn на карте

  section.typical.typical--filter
    .container.container--typical
      form(action="#")
        .typical__filter
          .typical__item
            p Этажность
            .typical__input
              .checkbox-container
                input#c1(type='checkbox' name='checkbox')
                label.label-container(for="c1")
                  span 1
                  
              .checkbox-container
                input#c2(type='checkbox' name='checkbox')
                label.label-container(for="c2")
                  span 2
          .typical__item
            p Площадь жилой части, м
              sup 2
            .typical__input
              .checkbox-container
                input#c11(type='checkbox' name='checkbox')
                label.label-container(for="c11")
                  span до 100
              .checkbox-container
                input#c12(type='checkbox' name='checkbox')
                label.label-container(for="c12")
                  span от 100 до 150
              .checkbox-container
                input#c13(type='checkbox' name='checkbox')
                label.label-container(for="c13")
                  span от 150
          .typical__item
            p Стоимость, р
            .typical__input
              .input-container(data-from="от")
                input(type="text" placeholder="600 000")
              .input-container(data-from="до")
                input(type="text" placeholder="20 000 000")
          .typical__item
            p Сортировка
            select(name="")
              option(value="По рейтингу и стоимости") Площадь вверх
              option(value="Рейтинг вниз") Площадь вниз
              option(value="Стоимость вверх") Стоимость вверх
              option(value="Стоимость вниз") Стоимость вниз
   
  // END SECTION :: typical

