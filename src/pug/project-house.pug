  // BEGIN SECTION :: head
  .head
    .container.container--large
      h2 Проект дома OPEN VILLAGE
      .head__buttons
        a.prev(href="#") 
          span.arrow
          span.info Все проекты

  // END SECTION :: head

  // BEGIN SECTION :: general
  section.general
    .container.container--large
      .general__grid
        .general__left
        .general__right
          p.general__title Основные характеристики
          .general__row
            p Общая площадь, м2
            p.bold 186
          .general__row
            p Срок строительства
            p.bold от 2 месяцев
          .general__row
            p Фундамент
            p.bold утеплённая шведская плита с контуром тёплого пола и колектором
          .general__row
            p Внешняя отделка
            .general__view
              a.general__view-item.is-active(href="#" js-general-view-btn)
                .general__view-image
                  img(src="img/g-view-1.jpg", alt="")
                p hauberG
              a.general__view-item(href="#" js-general-view-btn)
                .general__view-image
                  img(src="img/g-view-2.jpg", alt="")
                p Имитация бруса
              a.general__view-item(href="#" js-general-view-btn)
                .general__view-image
                  img(src="img/g-view-3.jpg", alt="")
                p Сайдинг
              a.general__view-item(href="#" js-general-view-btn)
                .general__view-image
                  img(src="img/g-view-4.jpg", alt="")
                p Штукатурка
              a.general__view-item(href="#" js-general-view-btn)
                .general__view-image
                  img(src="img/g-view-5.jpg", alt="")
                p Кирпичная кладка
          .general__row.general__row--price
            p Стоимость строительства:
            p.price от 1 500 000 р
          a.btn(href="#") заказать консультацию

